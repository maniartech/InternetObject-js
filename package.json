{
  "name": "internet-object",
  "version": "1.0.5-alpha.1",
  "description": "Internet Object Typescript/JavaScript library",
  "keywords": [],
  "author": {
    "name": "Mohamed Aamir Maniar",
    "email": "contact@maniartech.com",
    "url": "https://maniartech.com"
  },
  "license": "ISC",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist/*",
    "clean-build": "yarn clean && yarn build",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:watch": "tsc -w",
    "minify": "terser dist/index.js -o dist/index.min.js --compress --mangle --source-map",
    "lint": "echo 'Linting placeholder - add ESLint/Prettier later'",
    "type-check": "tsc --noEmit",
    "security:audit": "yarn audit",
    "security:audit-fix": "yarn audit fix",
    "security:check": "yarn audit --level moderate",
    "deps:check": "yarn outdated",
    "deps:update": "yarn upgrade",
    "precommit": "yarn lint && yarn type-check && yarn test",
    "prepare": "yarn build",
    "perf": "ts-node performance.ts || node dist/performance.js",
    "perf:decimal": "ts-node performance.decimal.ts || node dist/performance.decimal.js",
    "build:perf": "yarn build && yarn perf",
    "bundle:analyze": "bash scripts/bundle-analyze.sh",
    "bundle:test-minimal": "bash scripts/bundle-test-minimal.sh",
    "bundle:test-full": "bash scripts/bundle-test-full.sh",
    "bundle:compare": "bash scripts/bundle-compare.sh",
    "bundle:budget-check": "bash scripts/bundle-budget-check.sh"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "chalk": "^5.4.1",
    "esbuild": "^0.25.12",
    "jest": "^29.6.4",
    "terser": "^5.24.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
